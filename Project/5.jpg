$export = var_export($value, true);

if ("'" === $export[0] && $export !== $resolvedExport = $this->container->resolveEnvPlaceholders($export, "'.\$this->getEnv('%s').'")) {
$export = $resolvedExport;
if ("'" === $export[1]) {
$export = substr($export, 3);
}
if (".''" === substr($export, -3)) {
$export = substr($export, 0, -3);
}
}

return $export;
}
